<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
<package name="actions" namespace="/" extends="struts-default">	


<action name="login" class="actions.LoginAction">
	<result name="success"> indexAdmin.jsp </result>
	<result name="viajero"> indexViajero.jsp </result>
	<result name="input"> index.jsp </result>
</action>

<action name="registrarViajero" class="actions.RegistrarViajeroAction">
	<result name="success"> indexViajero.jsp </result>
	<result name="input"> registroViajero.jsp </result>
</action>

<action name="altaEventoAcademico" method="create" class="actions.CRUDEventoAcademicoAction">
	<result name="success" type="redirect"> listadoEventosAcademicos </result>
	<result name="input"> altaEventoAcademico.jsp </result>
</action>

<action name="listadoEventosAcademicos" method="list" class="actions.CRUDEventoAcademicoAction">
	<result name="success"> listadoEventosAcademicos.jsp </result>	
</action>

<action name="detailEventoAcademico" method="detail" class="actions.CRUDEventoAcademicoAction">
	<result name="success" > detalleEventoAcademico.jsp</result>	
</action>


</package>
</struts>